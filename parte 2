public class newproyect {

    public static void main(String[] args){
        coche puerta = new coche();

    }

    public static int suma(int a, int b, int c){
        return (a+b+c);
    }
    class coche{
        public int puerta = 0;
        public void Sumarpuerta(){
            this.puerta++;
            System.out.println(puerta);
        }
    }
}
